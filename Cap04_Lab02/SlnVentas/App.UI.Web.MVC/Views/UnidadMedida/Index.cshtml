@model IEnumerable<App.Entities.Base.UnidadMedida>
@{
    ViewBag.Title = "Index";
}

<h2>crear Unidad Medida</h2>

<p></p>
<table class="table">
    <tr>
        <th>Nombre</th>
        <th>Descripción</th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Nombre</td>
            <td>@item.Nombre</td>
            <td>
                <a href="@Url.Action("Edit", new { id = item.UnidadMedidaID})">
                    <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
                </a>

            </td>
        </tr>
    }


</table>

